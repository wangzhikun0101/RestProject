append itab_1.

itab_1-vbeln = '1000000009'.
itab_1-posnr = '200040'.
itab_1-brgew = '600'.
append itab_1.

clear itab.
clear g_itab.

types:begin of itab_2_1,
  vbeln like lips-vbeln,
  brgew like lips-brgew,
end of itab_2_1.

data itab_2 type standard table of itab_2_1 with header line.


loop at itab_1.          "把表2的交货凭证和毛重汇总给表三
  itab_2-vbeln = itab_1-vbeln.
  itab_2-brgew = itab_1-brgew.
  collect itab_2.
endloop.

loop at itab_2.
  g_itab-vbeln  = itab_2-vbeln .
  g_itab-btgew  = itab_2-brgew .
  append g_itab to itab.
endloop.

data itab_3 like itab with header line.

move itab to itab_3[].

loop at itab_3.
  case itab_3-vbeln.
    when '1000000009'.
      itab_3-lfdat = sy-datum.
    when '1000000005'.
      itab_3-lfdat = sy-datum  - 1.
    when '1000000003'.
      itab_3-lfdat = sy-datum  - 2.
    when '1000000002'.
      itab_3-lfdat = sy-datum  - 3.
    when '1000000001'.
      itab_3-lfdat = sy-datum  - 4.
  endcase.
  modify itab_3.

  if itab_3-lfdat+4(2) eq '08'.
    if itab_3-vbeln+9(1) eq '2'.
      delete itab_3.
    endif.
  endif.
endloop.

*delete itab_3 where lfdat+4(2) eq '08' and vbeln+9(1) eq '2'.

loop at itab_3.
  write: itab_3-vbeln ,
  itab_3-lfdat ,
  itab_3-btgew .
  skip.
endloop.

ranges:r_tab for likp-vbeln.
r_tab-sign = 'I'.
r_tab-low = '1000000001'.
r_tab-option = 'BT'.
r_tab-high = '1000000004'.
append r_tab.

write: r_tab-low,
       r_tab-high.